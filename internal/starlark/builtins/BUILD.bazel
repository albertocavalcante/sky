load("@rules_go//go:def.bzl", "go_library")

go_library(
    name = "builtins",
    srcs = ["provider.go"],
    importpath = "github.com/albertocavalcante/sky/internal/starlark/builtins",
    visibility = ["//:__subpackages__"],
    deps = ["//internal/starlark/filekind"],
)

filegroup(
    name = "data",
    srcs = glob(
        [
            "data/proto/**/*.pb",
            "data/proto/**/*.pbtxt",
        ],
        allow_empty = True,
    ),
    visibility = ["//:__subpackages__"],
)
